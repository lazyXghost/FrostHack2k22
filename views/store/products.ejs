<%- include('partials/header') %>
    <div class="pagetitle">
      <h1>Products</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/store/dashboard">Store</a></li>
          <li class="breadcrumb-item">Products</li>
        </ol>
      </nav>
    </div>

    <a href="/store/addProduct">Add new Product</a>
    <br>
    <br>
    <section class="section">
      <div class="row align-items-top">
        <% for(var i = 0;i<products.length;i++) {%>
            <div class="col-lg-3">
              <div class="card">
                <img src="/store_UI/img/card.jpg" class="card-img-top" alt="...">
                <div class="card-body">
                  <h5 class="card-title"><%= products[i].name %></h5>
                  <p class="card-text"><%= products[i].description %></p>
                </div>
              </div>
            </div>
        <% } %>
      </div>
    </section>
<%- include('partials/footer') %>